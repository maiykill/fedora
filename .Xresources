!---------- UI Scaling & Fonts ---------
! 96 is default for 1080p; increase for larger UI
Xft.dpi:        100
Xft.antialias:  true
! Xft.hinting:    true
! Xft.hintstyle:  hintslight
Xft.rgba:       rgb

!--------------- Colors (Rose Pine) ---------------
*background:   #232136
*foreground:   #e0def4
*cursorColor:  #e0def4

! Black
*color0:       #393552
*color8:       #6e6a86

! Red
*color1:       #eb6f92
*color9:       #eb6f92

! Green
*color2:       #9ccfd8
*color10:      #9ccfd8

! Yellow
*color3:       #f6c177
*color11:      #f6c177

! Blue
*color4:       #31748f
*color12:      #31748f

! Magenta/Purple
*color5:       #c4a7e7
*color13:      #c4a7e7

! Cyan
*color6:       #ebbcba
*color14:      #ebbcba

! White
*color7:       #e0def4
*color15:      #f6c177

!----------------- Xcursor (mouse pointer) ----------
Xcursor.theme: volantes_light_cursors
Xcursor.size: 38

